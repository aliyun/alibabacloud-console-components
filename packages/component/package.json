{
  "name": "@alicloud/console-components",
  "version": "1.2.0",
  "description": "Alibaba Cloud React Components",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/wind.js",
  "scripts": {
    "start": "NODE_ENV=development npx webpack-dev-server",
    "compile": "rm -rf dist/ && NODE_ENV=production COMPILE_OPTION=ignore_style_files npx webpack && rm dist/wind-noreset.js dist/wind-noreset.min.js",
    "babel": "rm -rf lib/ && NODE_ENV=production COMPILE_OPTION=ignore_style_files npx babel src/ --out-dir lib/_ --extensions \".js,.jsx,.ts,.tsx\"",
    "babel:esm": "rm -rf esm/ && NODE_ENV=production COMPILE_OPTION=ignore_style_files,esm npx babel src/ --out-dir esm/_ --extensions \".js,.jsx,.ts,.tsx\"",
    "make": "rm -rf types/ && node scripts/make.js",
    "build": "yarn run babel && yarn run babel:esm && yarn run make && yarn run compile && node scripts/generateCssWithoutIconFont.js",
    "prepare": "yarn run build",
    "publishInternal": "node ./scripts/publish-to-tnpm/index.js"
  },
  "keywords": [
    "react",
    "wind",
    "components",
    "design"
  ],
  "author": "MacroXing <macroxing@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@alicloud/xconsole-theme": "^1.1.0",
    "@alifd/next": "~1.21.1",
    "@babel/runtime-corejs2": "^7.7.4",
    "classnames": "^2.2.5",
    "hoist-non-react-statics": "^3.3.0",
    "lodash": "^4.17.5",
    "rc-form": "^2.2.1",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/parser": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.4",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.1.3",
    "css-loader": "^3.0.0",
    "ejs": "^2.5.8",
    "fast-sass-loader": "^1.5.0",
    "fetch-jsonp": "^1.1.3",
    "fs-extra": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.22.1",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-cropper": "^1.0.1",
    "react-dnd": "^8.0.0",
    "react-dnd-html5-backend": "^8.0.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recursive-readdir": "^2.2.2",
    "redux": "^4.0.0",
    "relative": "^3.0.2",
    "semver": "^6.3.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "yargs-parser": "^9.0.2"
  },
  "peerDependencies": {
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.css"
  ]
}
