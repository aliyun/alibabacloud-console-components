{
  "name": "@alicloud/console-components-slide-panel",
  "version": "2.2.4",
  "description": "React component for Alibaba Cloud.",
  "main": "lib/index.js",
  "typings": "es/index.d.ts",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wind",
    "wind-rc-rc-slide-panel",
    "react",
    "react component"
  ],
  "author": {
    "name": "csr632",
    "email": "632882184@qq.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "prepare": "npm run clean && npm run babel && npm run babel:esm && npm run types && npm run build",
    "clean": "rm -rf dist lib es doc-dist",
    "babel": "breezr build --engine babel",
    "babel:esm": "breezr build --engine babel --es-module",
    "types": "tsc -p src --emitDeclarationOnly",
    "build": "breezr build --engine webpack",
    "test": "breezr test:unit",
    "dev": "breezr start --config config/breezr-docs.config.ts",
    "doc:build": "rm -rf doc-dist && breezr build --config config/breezr-docs.config.ts",
    "upload": "OSS_NAME=@alicloud/console-components-slide-panel oss-upload",
    "upload:pre": "OSS_TAG=pre npm run upload"
  },
  "peerDependencies": {
    "@alicloud/console-components": "^1.0.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.x",
    "react": "^16.x",
    "react-dom": "^16.x",
    "styled-components": "^4.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@alicloud/console-components-page": "^1.0.14",
    "@alicloud/console-components-app-layout": "1.0.10",
    "@alicloud/console-components-console-menu": "1.0.9"
  }
}
